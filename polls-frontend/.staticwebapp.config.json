{
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "Content-Type": "text/javascript; charset=utf-8"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "Content-Type": "text/javascript; charset=utf-8"
      }
    },
    {
      "route": "/*.css",
      "headers": {
        "Content-Type": "text/css; charset=utf-8"
      }
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/ws/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/images/*", "*.{css,scss,js,png,gif,ico,jpg,svg,map}"]
  },
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript; charset=utf-8",
    ".mjs": "text/javascript; charset=utf-8",
    ".css": "text/css; charset=utf-8",
    ".wasm": "application/wasm",
    ".svg": "image/svg+xml",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".gif": "image/gif",
    ".ico": "image/x-icon",
    ".map": "application/json"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}

