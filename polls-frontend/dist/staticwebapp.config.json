{
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "Content-Type": "text/javascript; charset=utf-8"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "Content-Type": "text/javascript; charset=utf-8"
      }
    },
    {
      "route": "/*.css",
      "headers": {
        "Content-Type": "text/css; charset=utf-8"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "*.{css,js,png,gif,ico,jpg,svg,map}"]
  },
  "globalHeaders": {
    "cache-control": "public, max-age=3600"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript; charset=utf-8",
    ".mjs": "text/javascript; charset=utf-8",
    ".css": "text/css; charset=utf-8",
    ".svg": "image/svg+xml",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".ico": "image/x-icon",
    ".map": "application/json"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}

